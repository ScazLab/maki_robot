<launch>
	<arg name="model" default="$(find maki_robot)/maki.urdf" />
	<arg name="use_gui" default="True" />
	<include file="$(find urdf_tutorial)/launch/display.launch" >
		<arg name="model" value="$(arg model)" />
		<arg name="gui" value="$(arg use_gui)" />
	</include>
	<node name="makidemo1" pkg="maki_robot" type="MAKI-Arbotix-Interface.py"/>
	<node name="makidemo2" pkg="maki_robot" type="MAKI-URDF-Interface.py"/>	
</launch>
